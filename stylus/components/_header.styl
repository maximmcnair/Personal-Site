.header__main
  position fixed
  top 0px
  left 0px
  right 0px
  background #1E2E3D
  overflow hidden
  padding 15px 15px 12px 15px
  z-index 99999
  ul
    list-style-type none
    margin 0
    padding 0
  li
    margin 0
    padding 0
    float left

.header__main__nav
.header__main__social
  float left
  li
    margin-right 20px


.header__main__nav
  text-transform uppercase
  padding-top 2px

.header__main__social
  float right
  a
    font-size 20px